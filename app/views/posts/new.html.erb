<%= form_with model: @user, url: user_posts_path, method: :post, local: true, scope: :post do |f|  %>
  <div>
    <%= f.label :title %><br>
    <%= f.text_field :title%>
  </div>
  <div>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </div>
  <div>
    <%= f.submit "Post" %>
  </div>
<% end %>